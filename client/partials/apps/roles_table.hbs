<table class="table table-striped table-bordered main-table" >
    <thead>
    <tr>
        <th><input type="checkbox" name="tick" value="" id="checkAllRoles"></th>
        <th>Role</th>
        <th>Installed Device count</th>
        <th>Not-Installed Device count</th>
        <th>Total Device count</th>
    </tr>
    </thead>
    <tbody>
    {{#key_value this}}
        <tr>
            <td><input type="checkbox" name="tick" data-role="{{key}}" value=""></td>
            <td>{{key}}</td>
            <td>{{value.device_install_count}}</td>
            <td>{{value.device_not_install_count}}</td>
            <td>{{value.total_devices}}</td>
        </tr>
    {{/key_value}}
    </tbody>
</table>