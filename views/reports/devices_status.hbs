<div class="container">

	<div class="raw-fluid">
		<div class="span12">

			<div class="raw-fluid">
				<div class="block">
					<div class="block-header">
						<div class="block-header-title pull-left">
							Devices Compliance Monitoring {{#data.results.0.current_status}}(Current Status: {{this}}){{/data.results.0.current_status}}
						</div>
						
							<div class="pull-right">
									

								</div>

					</div>
					
					
					

					<div class="block-body block-body container-fluid raw-fluid">

						<div class="raw-fluid" style="margin-left: -25px">
							
							<form method="post" enctype="multipart/form-data">

							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputLastName">Start Date</label>
									<div class="controls">
										   
										 <div class="input-append date datepicker"  data-date="" data-date-format="yyyy-mm-dddd">
				<input id="startdate" name="startdate" class="span2" size="16" type="text" value="{{data.inputData.startdate}}"  >
				<span class="add-on"><i class="icon-th"></i></span>
			  </div>
									  
									</div>
								</div>
							</div>
							
							
							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputLastName">End Date</label>
									<div class="controls">
										   
										 <div class="input-append date datepicker"  data-date="" data-date-format="yyyy-mm-dddd">
				<input id="enddate" name="enddate" class="span2" size="16" type="text" value="{{data.inputData.enddate}}" >
				<span class="add-on"><i class="icon-th"></i></span>
			  </div>
									  
									</div>
								</div>
							</div>
							
							
							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputEmail">IMEI</label>
									<div class="controls">
										<input id="imei" class="span2" size="20" name="imei" type="text" value="{{data.inputData.imei}}" readonly >
										<input id="deviceid"  name="deviceid" type="hidden" value="{{data.inputData.deviceId}}"  >
									</div>
								</div>
							</div>

							
							<div class="span2 operation-filter">								
								<div class="control-group">
									<label class="control-label" for="inputPassword" >&nbsp;</label>
									<div class="controls">
										<button class="btn btn-orange" type="sumbit" >Find</button>
									</div>
								</div>
							</div>
							
							</form>
							
							
						</div>		
												

						<div class="raw">

							<div class="span11">						

								<table cellpadding="0" cellspacing="0" border="0"
								class="table table-striped table-bordered" id="main-table">
									<thead>
										<tr>
											<th>Username</th>
											<th>Date</th>
											<th>Description</th>
											<th>Status</th>											
										</tr>
									</thead>
									<tbody>
										{{#data.results}}
											<tr>
												<td>{{userID}}</td>
												<td>{{timeStamp}}</td>
												<td>{{#resons}}
													{{name}} {{#if status}}<span style="color:green">(Complience)</span>. {{else}}<span style="color:red">(Violated)</span>{{/if}}
													{{/resons}}</td>
												<td>{{#if status}}<span style="color:green">Complience</span>{{else}}<span style="color:red">Violated</span>{{/if}}</td>
											</tr>
										{{/data.results}}
									</tbody>									
								</table>
								
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>

	</div>

</div>

